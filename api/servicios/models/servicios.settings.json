{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "name": "Servicios"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Titulo": {
      "type": "string"
    },
    "Subtitulo": {
      "type": "string"
    },
    "Descripcion": {
      "type": "richtext"
    },
    "Imagen": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "id_service": {
      "type": "uid",
      "targetField": "Titulo"
    },
    "habilitado": {
      "type": "boolean"
    },
    "costo": {
      "type": "float"
    }
  }
}
